<div style="border-radius: 1px; border: 4px dotted #ffb6c1; font-size: 100%; padding: 20px;">
  <h1>Hello!!</h1>
<div>


    <h2>Log in Here</h2>

  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log in" %>
  <% end %>
